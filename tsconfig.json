{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "esModuleInterop": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "*": [
        "./*"
      ],
      "test/*": [
        "./test/*"
      ]
    },
    "types": [
      "node",
      "@wdio/globals/types",
      "@wdio/jasmine-framework",
      "expect-webdriverio/jasmine",
      "@types/jasmine"
    ]
  }
}
